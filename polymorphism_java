public class polymorphism
{
    public static void calculate(int a,int b)
    {
        int c = a+b;
        System.out.println("THE SUM OF THE GIVEN TWO NUMBERS IS "+c);
    }
    public static void calculate(double a,int b) {
        double p = a*b;
        System.out.println("THE PRODUCT OF THE GIVEN TWO NUMBERS IS "+p);
    }
    public static void calculate(int a,double b){
        double diff = a-b;
        System.out.println("THE DIFFERENCE OF THE GIVEN TWO NUMBERS IS "+diff);
    }
    public static void calculate(double a,double b)
    {
        double q = a/b;
        System.out.println("THE QUOTIENT OF THE GIVEN TWO NUMBERS IS "+q);
    }

    public static void main(String[] args) {
        polymorphism p = new polymorphism();
        p.calculate(4,5);
        p.calculate(5.5,6);
        p.calculate(5.5,6.5);
        p.calculate(6,3.5);
    }

}
